{
  "name": "ai-news-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run prisma:deploy:production && npm run prisma:generate:production && next build",
    "build:dev": "npm run prisma:generate:dev && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "npm run prisma:generate:auto",
    "prisma:generate:auto": "if [ \"$VERCEL\" = \"1\" ]; then npm run prisma:generate:production; else npm run prisma:generate:dev; fi",
    "prisma:generate:dev": "prisma generate --schema=prisma/schema.prisma",
    "prisma:generate:production": "prisma generate --schema=prisma/schema.postgresql.prisma",
    "prisma:deploy:production": "prisma migrate deploy --schema=prisma/schema.postgresql.prisma"
  },
  "dependencies": {
    "@prisma/adapter-better-sqlite3": "^7.1.0",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@radix-ui/react-slot": "^1.2.4",
    "@sparticuz/chromium": "^143.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/puppeteer": "^7.0.4",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "js-yaml": "^4.1.1",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.556.0",
    "next": "^16.0.7",
    "pg": "^8.16.3",
    "prisma": "^7.1.0",
    "puppeteer": "^24.32.0",
    "puppeteer-core": "^24.32.0",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "rss-parser": "^3.13.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24",
    "@types/pg": "^8.15.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
