{
  "name": "ai-news-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run prisma:deploy:production && npm run prisma:generate:production && next build",
    "build:dev": "npm run prisma:generate:dev && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "npm run prisma:generate:auto",
    "prisma:generate:auto": "if [ \"$VERCEL\" = \"1\" ]; then npm run prisma:generate:production; else npm run prisma:generate:dev; fi",
    "prisma:generate:dev": "prisma generate --schema=prisma/schema.prisma",
    "prisma:generate:production": "prisma generate --schema=prisma/schema.postgresql.prisma",
    "prisma:deploy:production": "prisma migrate deploy --schema=prisma/schema.postgresql.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@sparticuz/chromium": "^138.0.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/puppeteer": "^5.4.7",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.539.0",
    "next": "^15.5.2",
    "prisma": "^6.14.0",
    "puppeteer": "^24.17.0",
    "puppeteer-core": "^24.17.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "rss-parser": "^3.13.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5"
  }
}
